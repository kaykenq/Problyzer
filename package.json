{
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  },
  "name": "problyzer",
  "version": "0.0.1-beta",
  "description": "Probability analyzer designed to make predictions.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "test": "npm run build && node dist/index.js",
    "lint": "npx eslint --ext .js,.ts .",
    "format": "npx prettier --ignore-path .gitignore, dist --write \"**/*.+(js|ts|json)\"",
    "build": "npm run lint && npm run format && npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaykenq/problyzer.git"
  },
  "keywords": [
    "prediction",
    "analyzer",
    "dataanalyzer"
  ],
  "author": "kaykenq",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/kaykenq/problyzer/issues"
  },
  "homepage": "https://github.com/kaykenq/problyzer#readme"
}
